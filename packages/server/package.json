{
  "name": "pandaid-backend",
  "version": "1.0.0",
  "description": "pandaid-backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "docker:build": "docker build -t pandaid-backend .",
    "docker:run": "docker run -p 3000:3000 -d pandaid-backend",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.0.2",
    "@loopback/context": "^3.2.0",
    "@loopback/core": "^2.2.0",
    "@loopback/openapi-v3": "^3.1.1",
    "@loopback/repository": "^2.0.2",
    "@loopback/rest": "^3.1.0",
    "@loopback/rest-explorer": "^2.0.2",
    "@loopback/service-proxy": "^2.0.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.0.0",
    "source-map-support": "^0.5.16",
    "@loopback/testlab": "^2.0.2",
    "@types/node": "^10.17.17",
    "typescript": "~3.8.3"
  }
}
